{
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@bufbuild/jest-environment-jsdom": "^0.1.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/google.maps": "^3.58.1",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-test-renderer": "^19.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "turbo": "^2.4.2"
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:api": "cd apps/api && nest start --watch",
    "dev:client": "cd apps/client && vite",
    "build": "turbo run build",
    "start": "node apps/api/dist/main",
    "lint": "eslint . --ext .js,.ts,.tsx --fix",
    "test": "jest",
    "test:client": "jest apps/client/src",
    "test:api": "jest apps/api/src --coverage",
    "test:e2e": "jest --config apps/api/test/jest-e2e.json"
  },
  "workspaces": [
    "apps/*"
  ],
  "packageManager": "~npm@10.7.0",
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.0",
    "@vis.gl/react-google-maps": "^1.5.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.2.0"
  }
}
